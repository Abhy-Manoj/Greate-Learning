@model IEnumerable<myproject.Models.ViewAssignedProjects>

@{
    ViewBag.Title = "ViewAssignedProjects";
    Layout = "~/Views/Shared/_UserHomepage.cshtml";
}

@{
    string currentUser = (string)Session["Username"];

}
<h5>here is the Project assigned to you,<b> @currentUser </b></h5>
<hr />


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Employee_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Department)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Project_title)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Employee_name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Department)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Project_title)
                
            </td>

        </tr>
    }

</table>
